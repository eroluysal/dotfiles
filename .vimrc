filetype off
syntax on

filetype plugin indent on

set encoding=utf-8
set number
set nocompatible
set ruler

set wrap
set formatoptions=tcqrn1
set tabstop=2
set expandtab

set modelines=0
set visualbell

" Move up/down editor lines
nnoremap j gj
nnoremap k gk

set hidden
set ttyfast
set laststatus=2
set showmode
set showcmd
